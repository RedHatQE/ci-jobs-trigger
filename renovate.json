{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "openshift-python-utilities",
        "requests",
        "semver",
        "flask",
        "python-simple-logger",
        "shortuuid",
        "gitpython",
        "pygithub",
        "python-gitlab",
        "python-jenkins",
        "redhat-qe-cloud-tools",
        "pyaml-env",
        "ipdb",
        "ipython",
        "pytest",
        "pytest-cov",
        "pytest-mock"
      ],
      "groupName": "poetry-deps"
    }
  ]
}
